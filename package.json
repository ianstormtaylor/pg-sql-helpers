{
  "name": "pg-sql-helpers",
  "description": "A set helpers for writing dynamic SQL queries with `pg-sql` in Javascript.",
  "version": "0.2.1",
  "license": "MIT",
  "repository": "git://github.com/ianstormtaylor/pg-sql-helpers.git",
  "main": "./lib/index.js",
  "scripts": {
    "build": "babel ./src --out-dir ./lib",
    "clean": "rm -rf ./lib ./node_modules",
    "prepublish": "yarn run build",
    "release": "np",
    "test": "yarn run build && mocha --require babel-core/register ./test/index.js",
    "watch": "babel ./lib --out-dir ./lib --watch"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "mocha": "^3.2.0"
  },
  "keywords": [
    "dynamic",
    "escape",
    "interpolate",
    "orm",
    "pg",
    "pg-sql",
    "pgsql",
    "postgres",
    "postgresql",
    "query",
    "safe",
    "sql",
    "string",
    "template"
  ],
  "dependencies": {
    "is": "^3.2.1",
    "is-plain-object": "^2.0.4",
    "pg-sql": "^1.1.0"
  }
}
